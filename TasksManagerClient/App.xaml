<Application x:Class="TasksManagerClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TasksManagerClient"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                
                <ResourceDictionary Source="Dictionaryes\Brushes.xaml"/>
                <ResourceDictionary Source="Dictionaryes\Icons.xaml"/>

                <ResourceDictionary Source="Dictionaryes\Converters.xaml"/>

                <ResourceDictionary Source="Dictionaryes\Buttons.xaml"/>
                <ResourceDictionary Source="Dictionaryes\DialogWindow.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
